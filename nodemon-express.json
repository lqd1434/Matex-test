{
  "verbose": true,
  "ignore": [
    "asar",
    "release/**",
    ".cache",
    "node_modules/**"
  ],
  "watch": [
    "./electron/server/**"
  ],
  "execMap": {
    "ts": "ts-node"
  },
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "ts",
  "exec": "ts-node ./electron/server/start.ts"
}
