{
  "name": "matex",
  "version": "1.1.1",
  "description": "a toolbox app",
  "private": true,
  "author": "lqd1434",
  "license": "ISC",
  "keywords": [
    "electron",
    "Matex-app"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ncuhome/Matex.git"
  },
  "scripts": {
    "dev": "node ./scripts/watch.js",
    "dev:react": "vite",
    "dev:server": "nodemon --config nodemon-express.json",
    "dev:electron": "nodemon --config nodemon.json",
    "codgen": "npx playwright codegen http://localhost:3000/",
    "test:playwright": "ts-node test/preview.test.ts",
    "build": "concurrently \"npm run build:react\" \"npm run build:main_preload\"",
    "build:react": "vite build --config renderer/vite.config.ts",
    "build:main_preload": "cross-env NODE_ENV=production node ./scripts/build.js",
    "postbuild": "yarn test:playwright",
    "preview": "vite preview -c ./renderer/vite.config.ts",
    "postinstall": "electron-builder install-app-deps",
    "prerelease": "node scripts/copyFile.js",
    "release": "cross-env NODE_ENV=production electron-builder build --config electron_builder.json --publish never"
  },
  "devDependencies": {
    "@parcel/config-default": "^2.2.0",
    "@parcel/core": "^2.2.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@types/body-parser": "^1.19.2",
    "@types/color-string": "^1.5.0",
    "@types/express": "^4.17.13",
    "@types/history": "^4.7.9",
